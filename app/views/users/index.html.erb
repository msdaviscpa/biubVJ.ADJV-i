<!DOCTYPE html>
<% provide(:title, "User Index") %>

<html>
  
    
  <body>
    <h1><%= yield(:title) %></h1>
    
    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= link_to user.name, user %>,
          <%= link_to user.email, user %>
        </li>
      <% end %>
    </ul>
  
<%= link_to "Add a New User", '/users/new', class: "btn btn-primary" %>

  </body>
</html>